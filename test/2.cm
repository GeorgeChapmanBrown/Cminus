/*Prints the input if larger than 10, prints 10 otherwise*/
int addFour(int x){
	return x + 4;
}

void main (void) {
    int x;
    int y;
    x = 6;
    y = 7;  /*Ok up to here*/

    x = input();
    y = addFour(10);

    if(x > y){
    	output(x);
    }
    else{
    	output(y);
    }

    output(5000);

    return;
}