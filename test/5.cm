/* A program that uses Euclidâ€™s algorithm to 
compute gcd (All errors from 2.cm to 5.cm)*/

int gcd (int u, int v; int 5 ) {
    if (v == 0) 
        return u;
    else
        return gcd(v, u - u / v * v);  /* u-u/v*v == u mod v */
}

void main (void) {
    int x;
    int y
    x = input(x ==== 5); 
    y = input();
    output( gcd(x, y) );
}
